{
  "name": "vicphysio-gerenciador-de-fichas",
  "author": "Gabriel Brand√£o",
  "private": true,
  "version": "1.0.0",
  "description": "Gerenciador de fichas de pacientes para fisioterapeutas e fichas de alunos para instrutores de Pilates",
  "type": "module",
  "keywords": [],
  "license": "ISC",
  "scripts": {
    "electron:dev": "VITE_BUILD_TARGET='electron' vite",
    "electron:build": "tsc && VITE_BUILD_TARGET='electron' vite build && electron-builder",
    "electron:build-windows": "tsc && VITE_BUILD_TARGET='electron' vite build && electron-builder --win",
    "web:dev": "VITE_BUILD_TARGET='web' vite",
    "web:build": "tsc && VITE_BUILD_TARGET='web' vite build",
    "web:preview": "VITE_BUILD_TARGET='web' vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "test": "vitest"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "uuid": "^11.0.3"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/react": "^18.2.64",
    "@types/react-dom": "^18.2.21",
    "@typescript-eslint/eslint-plugin": "^8.22.0",
    "@typescript-eslint/parser": "^8.22.0",
    "@vitejs/plugin-react": "^4.2.1",
    "electron": "^30.0.1",
    "electron-builder": "^24.13.3",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "jsdom": "^25.0.1",
    "typescript": "^5.2.2",
    "vite": "^5.1.6",
    "vite-plugin-electron": "^0.28.6",
    "vite-plugin-electron-renderer": "^0.14.5",
    "vitest": "^2.1.5"
  },
  "main": "dist-electron/main.js"
}
